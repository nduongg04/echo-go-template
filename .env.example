# Server Configuration
PORT=8080
ENV=development

# JWT Configuration
JWT_SECRET=your-secret-key
JWT_EXPIRY_HOURS=72

# Database Configuration
DB_URL=postgres://postgres:postgres@localhost:5432/echo_store

# CORS Configuration
ALLOWED_ORIGINS=*
